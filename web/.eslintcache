ter-dom';\r\n\r\nimport mapIcon from '../utils/mapIcon';\r\nimport api from \"../services/api\";\r\n\r\nimport '../styles/pages/orphanage.css';\r\nimport SideBar from \"../components/Sidebar\";\r\n\r\ninterface Orphanage {\r\n  name: string;\r\n  latitude: number;\r\n  longitude: number;\r\n  about: string;\r\n  instructions: string;\r\n  opening_hours: string;\r\n  open_on_weekends: string;\r\n  images: Array<{\r\n    url: string;\r\n    id: number;\r\n  }>;\r\n}\r\n\r\ninterface OrphanageParams {\r\n  id: string;\r\n}\r\n\r\nexport default function Orphanage() {\r\n  const params = useParams<OrphanageParams>();\r\n  const [orphanage, setOrphanage] = useState<Orphanage>();\r\n  const [activeImageIndex, setActiveImageIndex] = useState(0);\r\n\r\n  useEffect(() => {\r\n    api.get(`orphanages/${params.id}`).then(response => {\r\n      setOrphanage(response.data);\r\n    })\r\n  }, [params.id]);\r\n\r\n  if(!orphanage){\r\n    return <p>Carregando...</p>\r\n  }\r\n  \r\n  return (\r\n    <div id=\"page-orphanage\">\r\n      <SideBar />\r\n\r\n      <main>\r\n        <div className=\"orphanage-details\">\r\n          <img src={orphanage.images[activeImageIndex].url} alt={orphanage.name} />\r\n\r\n          <div className=\"images\">\r\n            {orphanage.images.map((image, index) => {\r\n              return (\r\n                <button \r\n                  className={activeImageIndex === index ? 'active' : ''}\r\n                  type=\"button\" \r\n                  key={image.id}\r\n                  onClick={() => {\r\n                    setActiveImageIndex(index)\r\n                  }}\r\n                >\r\n                  <img src={image.url} alt={orphanage.name} />\r\n                </button>\r\n              )\r\n            })}\r\n          </div>\r\n          \r\n          <div className=\"orphanage-details-content\">\r\n            <h1>{orphanage.name}</h1>\r\n            <p>{orphanage.about}</p>\r\n\r\n            <div className=\"map-container\">\r\n              <MapContainer \r\n                center={[orphanage.latitude,orphanage.longitude]} \r\n                zoom={16} \r\n                style={{ width: '100%', height: 280 }}\r\n                dragging={false}\r\n                touchZoom={false}\r\n                zoomControl={false}\r\n                scrollWheelZoom={false}\r\n                doubleClickZoom={false}\r\n              >\r\n                <TileLayer url=\"https://a.tile.openstreetmap.org/{z}/{x}/{y}.png\"/>\r\n                \r\n                <Marker interactive={false} icon={mapIcon} position={[orphanage.latitude,orphanage.longitude]} />\r\n              </MapContainer>\r\n\r\n              <footer>\r\n                <a target=\"_blank\" rel=\"noopener\" href={`https://www.google.com/maps/dir/?api=1&destination=${orphanage.latitude},${orphanage.longitude}`}>Ver rotas no Google Maps</a>\r\n              </footer>\r\n            </div>\r\n\r\n            <hr />\r\n\r\n            <h2>Instruções para visita</h2>\r\n            <p>{orphanage.instructions}</p>\r\n\r\n            <div className=\"open-details\">\r\n              <div className=\"hour\">\r\n                <FiClock size={32} color=\"#15B6D6\" />\r\n                Segunda à Sexta <br />\r\n                {orphanage.opening_hours}\r\n              </div>\r\n              {orphanage.open_on_weekends ? (\r\n                <div className=\"open-on-weekends\">\r\n                <FiInfo size={32} color=\"#39CC83\" />\r\n                Atendemos <br />\r\n                fim de semana\r\n              </div>\r\n              ) : (\r\n                <div className=\"open-on-weekends dont-open\">\r\n                  <FiInfo size={32} color=\"#FF669D\" />\r\n                  Não Atendemos <br />\r\n                  fim de semana\r\n                </div>\r\n              )}\r\n            </div>\r\n\r\n            <button type=\"button\" className=\"contact-button\">\r\n              <FaWhatsapp size={20} color=\"#FFF\" />\r\n              Entrar em contato\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </main>\r\n    </div>\r\n  );\r\n}","C:\\Users\\andre\\Documents\\GitHub\\nlw-03-omnistack\\web\\src\\pages\\CreateOrphanage.tsx",["57"],"C:\\Users\\andre\\Documents\\GitHub\\nlw-03-omnistack\\web\\src\\index.tsx",[],"C:\\Users\\andre\\Documents\\GitHub\\Happy-nlw03-omnistack\\web\\src\\index.tsx",[],["58","59"],"C:\\Users\\andre\\Documents\\GitHub\\Happy-nlw03-omnistack\\web\\src\\pages\\Landing.tsx",[],"C:\\Users\\andre\\Documents\\GitHub\\Happy-nlw03-omnistack\\web\\src\\pages\\OrphanagesMap.tsx",["60"],"C:\\Users\\andre\\Documents\\GitHub\\Happy-nlw03-omnistack\\web\\src\\components\\Sidebar.tsx",[],"C:\\Users\\andre\\Documents\\GitHub\\Happy-nlw03-omnistack\\web\\src\\pages\\CreateOrphanage.tsx",["61"],"C:\\Users\\andre\\Documents\\GitHub\\Happy-nlw03-omnistack\\web\\src\\routes.tsx",["62"],"C:\\Users\\andre\\Documents\\GitHub\\Happy-nlw03-omnistack\\web\\src\\pages\\RestrictArea.tsx",[],{"ruleId":"63","severity":1,"message":"64","line":5,"column":8,"nodeType":"65","messageId":"66","endLine":5,"endColumn":15},{"ruleId":"67","severity":1,"message":"68","line":31,"column":25,"nodeType":"65","messageId":"69","endLine":31,"endColumn":34},{"ruleId":"63","severity":1,"message":"70","line":33,"column":46,"nodeType":"65","messageId":"66","endLine":33,"endColumn":55},{"ruleId":"71","severity":1,"message":"72","line":92,"column":20,"nodeType":"73","endLine":92,"endColumn":35},{"ruleId":"63","severity":1,"message":"74","line":3,"column":29,"nodeType":"65","messageId":"66","endLine":3,"endColumn":51},{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"63","severity":1,"message":"64","line":5,"column":8,"nodeType":"65","messageId":"66","endLine":5,"endColumn":15},{"ruleId":"63","severity":1,"message":"74","line":3,"column":29,"nodeType":"65","messageId":"66","endLine":3,"endColumn":51},{"ruleId":"63","severity":1,"message":"79","line"